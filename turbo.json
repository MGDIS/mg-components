{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", "loader/**", "www/**", ".storybook/docs/**"]
    },
    "storybook:build": {
      "outputs": ["storybook-static/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": ["dist/**", "loader/**", "www/**", ".storybook/docs/**", "coverage/**"]
    }
  }
}
