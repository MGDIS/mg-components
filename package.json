{
  "name": "@mgdis/mg-components",
  "version": "5.7.0",
  "description": "MG Components",
  "packageManager": "pnpm@7.17.1",
  "engineStrict": true,
  "engines": {
    "pnpm": ">=7"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mg-components/mg-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "maintainers": [
    "Simon Duhem <duhem-s@mgdis.fr>",
    "Nicolas Jouanno <jouanno-n@mgdis.fr>"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "lint": "run-s lint:es lint:prettier",
    "lint:fix": "run-s lint:es:fix lint:prettier:fix",
    "lint:es": "eslint src/**/*.{ts,tsx}",
    "lint:es:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:prettier": "prettier --check **/*.{ts,tsx,scss,html,mdx,json}",
    "lint:prettier:fix": "prettier --write **/*.{ts,tsx,scss,html,mdx,json}",
    "test": "TZ=UTC stencil test --spec --e2e",
    "test:unit": "TZ=UTC stencil test --spec --collectCoverage --reporters=\"default\" --reporters=\"jest-junit\"",
    "test:e2e": "stencil test --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "storybook:build": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "prepare": "husky install"
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "junit.xml"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@stencil/core": "^3.0.0",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-react-jsx": "^7.20.13",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/html": "^6.5.16",
    "@storybook/preset-scss": "^1.0.3",
    "@types/jest": "^26.0.24",
    "@types/jest-image-snapshot": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "chromatic": "^6.15.0",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-plugin-jsdoc": "^38.1.6",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-image-snapshot": "^4.5.1",
    "jest-junit": "^13.2.0",
    "jsx-dom": "^8.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "puppeteer": "^13.7.0",
    "sass": "^1.58.0",
    "sass-loader": "^10.4.1",
    "style-loader": "^2.0.0",
    "ts-node": "^10.9.1",
    "turbo": "^1.7.3",
    "typescript": "~4.4.4"
  },
  "license": "BSD-3-Clause"
}
