{
  "name": "@mgdis/mg-components",
  "version": "5.3.0",
  "description": "MG Components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mg-components/mg-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "maintainers": [
    "Simon Duhem <duhem-s@mgdis.fr>",
    "Nicolas Jouanno <jouanno-n@mgdis.fr>"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "lint": "run-s lint:es lint:prettier",
    "lint:fix": "run-s lint:es:fix lint:prettier:fix",
    "lint:es": "eslint src/**/*.{ts,tsx,js}",
    "lint:es:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:prettier": "prettier --check **/*.{ts,tsx,scss,html,mdx,json}",
    "lint:prettier:fix": "prettier --write **/*.{ts,tsx,scss,html,mdx,json}",
    "test": "TZ=UTC stencil test --spec --e2e",
    "test:unit": "TZ=UTC stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prepare": "husky install"
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "junit.xml"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "@stencil/core": "^2.18.1",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/html": "^6.5.13",
    "@storybook/preset-scss": "^1.0.3",
    "@types/jest": "^26.0.24",
    "@types/jest-image-snapshot": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "chromatic": "^6.10.1",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-plugin-jsdoc": "^38.0.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-storybook": "^0.6.6",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-image-snapshot": "^4.5.1",
    "jest-junit": "^13.0.0",
    "jsx-dom": "^8.0.1-beta.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "puppeteer": "^13.3.2",
    "sass": "^1.41.0",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.3"
  },
  "license": "BSD-3-Clause"
}
