    "jest-image-snapshot": "^4.5.1",
    "jest-junit": "^13.0.0",
    "jsx-dom": "^8.0.1-beta.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "puppeteer": "^13.3.2",
    "sass": "^1.41.0",
